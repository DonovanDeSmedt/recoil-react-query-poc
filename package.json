{
  "name": "mwi-react-template",
  "description": "delaware MWI React boilerplate",
  "version": "5.0.0",
  "private": true,
  "dependencies": {
    "@dlw/dlw-authenticator": "0.0.9",
    "@dlw/dlw-logger": "0.0.6",
    "axios": "^0.19.2",
    "bfj": "6.1.1",
    "jwt-decode": "2.2.0",
    "react": "16.9.0",
    "react-app-polyfill": "0.2.1",
    "react-dom": "16.8.3",
    "react-query": "^1.5.2",
    "react-redux": "6.0.1",
    "recoil": "^0.0.7",
    "redux": "4.0.4",
    "redux-actions": "2.6.5",
    "redux-saga": "1.0.5",
    "redux-thunk": "2.3.0",
    "resolve": "1.10.0",
    "styled-components": "4.1.3",
    "uuid": "3.3.2",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.3",
    "babel-preset-react-app": "7.0.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "css-loader": "3.2.0",
    "dotenv": "6.0.0",
    "eslint": "5.12.0",
    "eslint-config-prettier": "5.1.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "3.11.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.6.0",
    "file-loader": "4.2.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": ">=3",
    "jest": "24.8.0",
    "lint-staged": ">=8",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "1.18.2",
    "react-dev-utils": "9.0.1",
    "react-test-renderer": "^16.8.6",
    "redux-logger": "3.0.6",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.4.1",
    "url-loader": "2.1.0",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "dist": "node scripts/build.js",
    "addRedux": "node scripts/redux/addRedux.js",
    "test": "jest --bail --passWithNoTests",
    "test:watch": "jest --bail --watch",
    "test:report": "jest --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html",
    "runYarnLock": "yarn install --frozen-lockfile"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*-reducers.js",
      "src/**/*-reducer.js",
      "src/**/*-selectors.js",
      "src/**/*-selector.js",
      "src/**/*-sagas.js",
      "src/**/*-saga.js",
      "src/**/utils/helpers.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/config/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/modules"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn test",
      "pre-push": " yarn test",
      "post-rewrite": "yarn",
      "post-checkout": "yarn",
      "post-merge": "yarn runYarnLock",
      "post-rebase": "yarn install"
    }
  },
  "lint-staged": {
    "*.{js,css, jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  }
}
