{
  "name": "mwi-react-template",
  "description": "delaware MWI React boilerplate",
  "version": "5.0.0",
  "private": true,
  "dependencies": {
    "@dlw/dlw-authenticator": "0.0.1",
    "@dlw/dlw-logger": "0.0.6",
    "bfj": "6.1.1",
    "jwt-decode": "^2.2.0",
    "react": "^16.8.3",
    "react-app-polyfill": "^0.2.1",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "resolve": "1.10.0",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "^2.4.2",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "eslint": "5.12.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "jest": "23.6.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "react-dev-utils": "^8.0.0",
    "redux-logger": "^3.0.6",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.4.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "dist": "node scripts/build.js",
    "addRedux": "node scripts/redux/addRedux.js",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*-reducers.js",
      "src/**/*-reducer.js",
      "src/**/*-selectors.js",
      "src/**/*-selector.js",
      "src/**/*-sagas.js",
      "src/**/*-saga.js",
      "src/**/utils/helpers.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/config/"
    ],
    "testResultsProcessor": "./node_modules/jest-junit",
    "moduleDirectories": [
      "node_modules",
      "src/modules"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
